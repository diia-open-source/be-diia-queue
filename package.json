{
    "name": "@diia-inhouse/diia-queue",
    "version": "5.10.4",
    "description": "Package provide queue functionality",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "repository": "https://github.com/diia-open-source/be-diia-queue.git",
    "author": "Diia",
    "license": "SEE LICENSE IN LICENSE.md",
    "engines": {
        "node": ">=18"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist/ && tsc",
        "start": "node dist/index.js",
        "lint": "eslint --ext .ts . && prettier --check .",
        "lint-fix": "eslint --ext .ts --fix && prettier --write .",
        "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts registry.npmjs.org --validate-https",
        "format": "prettier --write \"src/**/*.ts\"",
        "prepare": "npm run build",
        "semantic-release": "semantic-release",
        "test": "jest",
        "test:integration": "npm run test --selectProjects integration --",
        "test:unit": "npm run test --selectProjects unit --",
        "test:coverage": "npm run test --selectProjects unit --coverage",
        "find-circulars": "madge --circular --extensions ts ./"
    },
    "dependencies": {
        "@opentelemetry/api": "1.7.0",
        "@types/amqplib": "0.10.4",
        "amqplib": "0.10.3",
        "lodash": "4.17.21",
        "uuid": "9.0.1"
    },
    "peerDependencies": {
        "@diia-inhouse/diia-metrics": ">=3.6.0",
        "@diia-inhouse/errors": ">=1.4.0",
        "@diia-inhouse/redis": ">=2.1.1",
        "@diia-inhouse/types": ">=5.0.0",
        "@diia-inhouse/utils": ">=2.21.0",
        "@diia-inhouse/validators": ">=1.5.0"
    },
    "devDependencies": {
        "@diia-inhouse/configs": "^1.26.3",
        "@diia-inhouse/crypto": "^1.7.0",
        "@diia-inhouse/diia-logger": "^2.10.0",
        "@diia-inhouse/diia-metrics": "^3.6.0",
        "@diia-inhouse/env": "^1.4.0",
        "@diia-inhouse/errors": "^1.6.1",
        "@diia-inhouse/eslint-config": "^3.4.0",
        "@diia-inhouse/eslint-plugin": "^1.3.3",
        "@diia-inhouse/redis": "^2.4.0",
        "@diia-inhouse/test": "^5.1.0",
        "@diia-inhouse/types": "^5.0.0",
        "@diia-inhouse/utils": "^2.33.1",
        "@diia-inhouse/validators": "^1.12.1",
        "@types/lodash": "4.14.202",
        "@types/node": "20.11.5",
        "@types/uuid": "9.0.7",
        "lockfile-lint": "4.12.1",
        "madge": "6.1.0",
        "protobufjs": "7.2.5",
        "type-fest": "4.8.2"
    },
    "release": {
        "extends": "@diia-inhouse/configs/dist/semantic-release/package"
    },
    "commitlint": {
        "extends": "@diia-inhouse/configs/dist/commitlint"
    },
    "eslintConfig": {
        "extends": "@diia-inhouse/eslint-config",
        "rules": {
            "@typescript-eslint/no-unnecessary-condition": [
                "off"
            ]
        },
        "overrides": [
            {
                "files": [
                    "*.ts"
                ],
                "parserOptions": {
                    "project": [
                        "./tsconfig.json",
                        "./tests/tsconfig.json"
                    ]
                }
            }
        ]
    },
    "jest": {
        "preset": "@diia-inhouse/configs/dist/jest"
    },
    "prettier": "@diia-inhouse/eslint-config/prettier",
    "madge": {
        "tsConfig": "./tsconfig.json"
    }
}
